{
    "info": {
        "_postman_id": "insurance-agent-api",
        "name": "Insurance Agent API",
        "description": "API for triggering ElevenLabs phone calls to insurers",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Health Check",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{base_url}}/health",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "health"
                    ]
                },
                "description": "Check if the API is running"
            },
            "response": []
        },
        {
            "name": "Call Insurer",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"phone_number\": \"{{phone_number}}\"\n}"
                },
                "url": {
                    "raw": "{{base_url}}/api/calls/insurer",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "calls",
                        "insurer"
                    ]
                },
                "description": "Trigger an ElevenLabs phone call to an insurer"
            },
            "response": []
        },
        {
            "name": "Call Insurer (Custom Agent)",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"phone_number\": \"{{phone_number}}\",\n  \"agent_id\": \"{{agent_id}}\"\n}"
                },
                "url": {
                    "raw": "{{base_url}}/api/calls/insurer",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "calls",
                        "insurer"
                    ]
                },
                "description": "Trigger an ElevenLabs phone call with a specific agent ID"
            },
            "response": []
        },
        {
            "name": "Get All Transcripts",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{base_url}}/api/calls/transcripts?limit=100&offset=0",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "calls",
                        "transcripts"
                    ],
                    "query": [
                        {
                            "key": "limit",
                            "value": "100",
                            "description": "Maximum number of transcripts to return"
                        },
                        {
                            "key": "offset",
                            "value": "0",
                            "description": "Number of transcripts to skip"
                        }
                    ]
                },
                "description": "Retrieve all call transcripts with pagination"
            },
            "response": []
        },
        {
            "name": "Get Transcript by ID",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{base_url}}/api/calls/transcripts/:conversation_id",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "api",
                        "calls",
                        "transcripts",
                        ":conversation_id"
                    ],
                    "variable": [
                        {
                            "key": "conversation_id",
                            "value": "",
                            "description": "The conversation ID to retrieve"
                        }
                    ]
                },
                "description": "Retrieve a specific call transcript by conversation ID"
            },
            "response": []
        }
    ]
}